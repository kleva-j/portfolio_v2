{
  "name": "portfolio_v2",
  "version": "1.0.0",
  "description": "portfolio app version 2",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kleva-j/portfolio@v2.git"
  },
  "keywords": [
    "react",
    "vite",
    "typescript",
    "template",
    "ghactions",
    "vitest"
  ],
  "author": "kleva-j",
  "license": "MIT",
  "homepage": "https://github.com/kleva-j/portfolio_v2",
  "packageManager": "pnpm@8.2.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:ci": "tsc && vite build && cp 404.html dist/",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ci": "vitest run --coverage",
    "serve": "vite preview",
    "prepare": "husky install && rm -rf .git/hooks && ln -s ../.husky .git/hooks",
    "preview": "vite preview --port 8080",
    "deploy-gh-pages": "git push origin :gh-pages && git subtree push --prefix dist origin gh-pages",
    "lint": "eslint --cache --fix ./src"
  },
  "dependencies": {
    "@emailjs/browser": "^3.10.0",
    "@react-three/drei": "^9.58.4",
    "@react-three/fiber": "^8.12.0",
    "@tanstack/react-query": "^4.28.0",
    "@tanstack/react-query-devtools": "^4.28.0",
    "@tanstack/react-router": "0.0.1-beta.83",
    "@tanstack/react-router-devtools": "0.0.1-beta.83",
    "@tanstack/router": "0.0.1-beta.83",
    "@tanstack/router-devtools": "0.0.1-alpha.1",
    "axios": "^1.3.4",
    "framer-motion": "^10.9.1",
    "maath": "^0.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-tilt": "^1.0.1",
    "react-vertical-timeline-component": "^3.6.0",
    "three": "^0.151.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@commitlint/prompt-cli": "^17.0.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.9",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@types/node": "^18.6.4",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-vertical-timeline-component": "^3.3.3",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "@vitejs/plugin-react": "^2.0.0",
    "autoprefixer": "^10.4.14",
    "c8": "^7.12.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.6.0",
    "happy-dom": "^6.0.4",
    "husky": "^8.0.1",
    "lint-staged": "^11.2.3",
    "postcss": "^8.4.21",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.6.4",
    "vite": "^3.0.0",
    "vite-tsconfig-paths": "^3.5.0",
    "vitest": "^0.21.0"
  }
}
